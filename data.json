{
  "inboxFolder": "_Inbox",
  "interval": 60,
  "convertExtensionsToLowercase": true,
  "rules": [
    {
      "regex": "^\\d{8}_\\d{6}",
      "targetFolder": "",
      "fileExtensions": "jpg|png",
      "rootFolder": "Chronological/Photos",
      "folderStructure": "YYYY/YYYY-MM"
    },
    {
      "regex": "^\\d{8} - ",
      "targetFolder": "Chronological/Documents",
      "fileExtensions": "txt|htm|html|pdf",
      "rootFolder": "Chronological/Documents",
      "folderStructure": "YYYY/YYYY-MM"
    },
    {
      "regex": "^\\d{8}_\\d{6} - *",
      "fileExtensions": "jpg",
      "rootFolder": "Chronological/Private",
      "folderStructure": "YYYY/YYYY-MM"
    }
  ],
  "documentsEnabled": true,
  "documentsFolder": "Chronological/Documents",
  "documentsStructure": "YYYY/YYYY-MM",
  "documentsFileType": "txt|htm|html|pdf|PDF",
  "documentsFileRegex": "^\\d{8} - .*",
  "photosEnabled": true,
  "photosFolder": "Chronological/Photos",
  "photosStructure": "YYYY/YYYY-MM",
  "photosFileType": "jpg|png",
  "photosFileRegex": "^\\d{8}_\\d{6}.*"
}